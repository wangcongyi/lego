  "scripts": {
    "version": "echo $npm_package_version",
    "dockerStart": "./bin/docker.sh $npm_package_version",
    "start": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json --env production",
    "debug-server": "cross-env DEBUG=* NODE_ENV=development node --inspect server.js",
    "prod": "run-p prod:*",
    "prod:server": "yarn server:prod",
    "prod:client": "webpack -w",
    "dev": "run-p dev:*",
    "dev:client": "webpack -w",
    "dev:server": "yarn server:dev",
    "devp": "run-p devp:*",
    "devp:client": "rimraf dist && cross-env BUILD_ARGVS=dev NODE_ENV=production webpack -w",
    "devp:server": "yarn server:devp",
    "server:devp": "cross-env DEBUG=app* BUILD_ARGVS=dev NODE_ENV=production node server.js",
    "server:dev": "cross-env DEBUG=app* BUILD_ARGVS=dev NODE_ENV=development node server.js",
    "server:uat": "cross-env DEBUG=app* BUILD_ARGVS=uat NODE_ENV=production node server.js",
    "server:fat": "cross-env DEBUG=app* BUILD_ARGVS=fat NODE_ENV=production node server.js",
    "server:prod": "cross-env DEBUG=app* BUILD_ARGVS=prod NODE_ENV=production node server.js",
    "build:dev": "rimraf dist && cross-env BUILD_ARGVS=dev NODE_ENV=development webpack",
    "build:devp": "rimraf dist && cross-env BUILD_ARGVS=dev NODE_ENV=production webpack",
    "build:uat": "rimraf dist && cross-env BUILD_ARGVS=uat NODE_ENV=production webpack",
    "build:fat": "rimraf dist && cross-env BUILD_ARGVS=fat NODE_ENV=production webpack",
    "build:prod": "rimraf dist && cross-env BUILD_ARGVS=prod NODE_ENV=production webpack",
    "generateAnalyzFile": "webpack --profile --json > ./dist/stats.json",
    "analyz": "webpack-bundle-analyzer --port 8888 ./dist/stats.json"
